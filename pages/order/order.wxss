.tabs {
  display: flex;
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  overflow: auto;
  background-color: #fff;
}
.zhengge {
  padding-top: 70rpx;
}
.zhengge .order {
  margin-top: 10rpx;
  padding: 10rpx 30rpx;
  border-bottom: 1px solid #eee;
}
.zhengge .order .yi,
.zhengge .order .er,
.zhengge .order .san {
  display: flex;
  justify-content: space-between;
  color: #6f6f72;
  font-size: 28rpx;
}
.zhengge .order .er {
  margin-top: 10rpx;
}
.zhengge .order .er .jiage {
  color: #df4848;
}
.zhengge .order .san {
  margin-top: 10rpx;
}
.zhengge .order .san .riqi {
  color: #444442;
}
